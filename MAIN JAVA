// Classe Main public class Main {
public static void main(String[] args) {
ContaBancaria conta1 = new ContaBancaria("12345", "Lorena"); ContaBancaria conta2 = new ContaBancaria("67890", "Renan");

conta1.depositar(500); conta1.sacar(200); conta1.transferir(conta2, 100);

System.out.println("Saldo da conta 1: " + conta1.getSaldo()); System.out.println("Saldo da conta 2: " + conta2.getSaldo());
}
}
